<div class="form-row">
  <%= form_with(model: group, local: true)do |f| %>
    <div class="form-group">
      <%= f.hidden_field :user_id, multiple: true %>
    </div>
    <div class="form-group">
      <%= f.label :group_name %>
      <%= f.text_field :group_name %>
    </div>
    <div class="form-group">
      <%= f.submit :class => 'btn btn-primary' %>
    </div>
  <% end %>
</div>
